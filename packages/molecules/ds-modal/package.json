{
  "name": "@karinmelissa/ds-modal",
  "description": "Modal component",
  "version": "1.0.0",
  "scripts": {
    "start": "vite",
    "build:types": "tsc -p tsconfig.build.json",
    "build:compile": "library \"src/**/*\"",
    "build:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers",
    "build": "npm run build:types && npm run build:compile && npm run build:exports",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run test:coverage && npm run exports"
  },
  "dependencies": {
    "@atomico/hooks": "^4.0.0",
    "@uikit/ds-button": "^1.0.0",
    "atomico": "latest"
  },
  "devDependencies": {
    "@atomico/exports": "^1.13.2",
    "@atomico/tsconfig": "^1.1.2",
    "@atomico/vite": "^2.7.0",
    "@uppercod/vite-meta-url": "latest",
    "@uppercod/vite-meta-url-load-css": "latest",
    "csso": "latest",
    "jsdom": "^26.1.0",
    "typescript": "^4.8.4",
    "vite": "latest",
    "vite-plugin-istanbul": "^3.0.1",
    "vitest": "^3.1.4"
  },
  "peerDependencies": {
    "@atomico/react": "*",
    "@atomico/vue": "*"
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    },
    "@atomico/vue": {
      "optional": true
    }
  },
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./lib/index.js"
    },
    "./index.react": "./dist/index.react.js",
    "./index.visibility": "./dist/index.visibility.css",
    "./index": "./dist/index.js",
    "./modal": "./dist/components/modal.js",
    "./index.preact": "./dist/index.preact.js",
    "./react": {
      "types": "./wrapper/react.d.ts",
      "default": "./wrapper/react.js"
    },
    "./preact": {
      "types": "./wrapper/preact.d.ts",
      "default": "./wrapper/preact.js"
    },
    "./ds-modal": {
      "default": "./lib/ds-modal.mjs"
    },
    "./modal.styles": {
      "types": "./types/modal.styles.d.ts",
      "default": "./lib/modal.styles.js"
    },
    "./components/modal": {
      "types": "./types/components/modal.d.ts",
      "default": "./lib/components/modal.js"
    },
    "./vue": {
      "types": "./wrapper/vue.d.ts",
      "default": "./wrapper/vue.js"
    },
    "./components/Modal": {
      "default": "./lib/components/Modal.js"
    }
  },
  "typesVersions": {
    "*": {
      "index.react": [
        "types/index.react.d.ts"
      ],
      "index": [
        "./types/index.d.ts"
      ],
      "modal": [
        "types/modal.d.ts"
      ],
      "index.preact": [
        "types/index.preact.d.ts"
      ],
      "react": [
        "wrapper/react.d.ts"
      ],
      "preact": [
        "wrapper/preact.d.ts"
      ],
      "modal.styles": [
        "types/modal.styles.d.ts"
      ],
      "components/modal": [
        "types/components/modal.d.ts"
      ],
      "vue": [
        "wrapper/vue.d.ts"
      ]
    }
  },
  "types": "types/index.d.ts"
}
