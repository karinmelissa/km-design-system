{
  "name": "@ui-kit/ds-input",
  "description": "Input component",
  "version": "1.0.0",
  "scripts": {
    "start": "vite",
    "build:types": "tsc -p tsconfig.build.json",
    "build:compile": "library \"src/**/*\"",
    "build:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers",
    "build": "npm run build:types && npm run build:compile && npm run build:exports",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@atomico/hooks": "^4.0.0",
    "atomico": "latest"
  },
  "devDependencies": {
    "@atomico/exports": "^0.37.0",
    "@atomico/tsconfig": "^1.1.2",
    "@atomico/vite": "^2.7.0",
    "@open-wc/testing": "^3.1.6",
    "@uppercod/vite-meta-url": "latest",
    "@uppercod/vite-meta-url-load-css": "latest",
    "@vitest/coverage-istanbul": "^0.30.1",
    "@vitest/ui": "^0.30.1",
    "csso": "latest",
    "jsdom": "^21.1.1",
    "typescript": "^4.8.4",
    "vite": "^4.2.1",
    "vite-plugin-istanbul": "^3.0.1"
  },
  "peerDependencies": {
    "@atomico/react": "*",
    "@atomico/vue": "*"
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    },
    "@atomico/vue": {
      "optional": true
    }
  },
  "module": "./dist/index.js",
  "exports": {
    "./index.react": "./dist/index.react.js",
    "./index.visibility": "./dist/index.visibility.css",
    "./index": "./dist/index.js",
    ".": {
      "types": "./types/index.d.ts",
      "default": "./lib/index.js"
    },
    "./input": "./dist/input.js",
    "./Input": "./dist/components/Input.js",
    "./index.preact": "./dist/index.preact.js",
    "./preact": {
      "types": "./wrapper/preact.d.ts",
      "default": "./wrapper/preact.js"
    },
    "./react": {
      "types": "./wrapper/react.d.ts",
      "default": "./wrapper/react.js"
    },
    "./InputI": "./dist/components/InputI.js",
    "./types": "./dist/components/types.js",
    "./css/input.style": {
      "types": "./types/css/input.style.d.ts",
      "default": "./lib/css/input.style.js"
    },
    "./components/Input": {
      "types": "./types/components/Input.d.ts",
      "default": "./lib/components/Input.js"
    },
    "./components/Input.type": {
      "types": "./types/components/Input.type.d.ts",
      "default": "./lib/components/Input.type.js"
    },
    "./components/types": {
      "types": "./types/components/types.d.ts",
      "default": "./lib/components/types.js"
    },
    "./vue": {
      "types": "./wrapper/vue.d.ts",
      "default": "./wrapper/vue.js"
    }
  },
  "typesVersions": {
    "*": {
      "index.react": [
        "./types/index.react.d.ts"
      ],
      "Input": [
        "./types/components/Input.d.ts"
      ],
      "index": [
        "./types/index.d.ts"
      ],
      "index.preact": [
        "./types/index.preact.d.ts"
      ],
      "react": [
        "wrapper/react.d.ts"
      ],
      "preact": [
        "wrapper/preact.d.ts"
      ],
      "InputI": [
        "./types/components/InputI.d.ts"
      ],
      "types": [
        "./types/components/types.d.ts"
      ],
      "components/Input": [
        "types/components/Input.d.ts"
      ],
      "components/Input.spec": [
        "types/components/Input.spec.d.ts"
      ],
      "components/Input.type": [
        "types/components/Input.type.d.ts"
      ],
      "components/types": [
        "types/components/types.d.ts"
      ],
      "css/input.style": [
        "types/css/input.style.d.ts"
      ],
      "vue": [
        "wrapper/vue.d.ts"
      ]
    }
  },
  "types": "types/index.d.ts"
}